{
  "name": "template-sync-server",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/local-first/sync-server",
  "projectType": "application",
  "targets": {
    "app-build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/local-first/sync-server"],
      "options": {
        "parallel": false,
        "command": "pnpm xdev build --cwd apps/local-first/sync-server"
      }
    },
    "app-deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm xdev deploy --cwd apps/local-first/sync-server"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm xdev serve --cwd apps/local-first/sync-server"
      }
    },
    "madge": {
      "executor": "nx:run-commands",
      "options": {
        "command": "madge --ts-config ./tsconfig.app.json --circular --no-color --no-spinner --extensions ts,tsx .",
        "cwd": "apps/local-first/sync-server"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsgo -p apps/local-first/sync-server/tsconfig.json"
      }
    },
    "db": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "seed",
      "configurations": {
        "seed": {
          "command": "pnpm xdev db seed --cwd apps/local-first/sync-server"
        },
        "push": {
          "command": "pnpm xdev db push --cwd apps/local-first/sync-server"
        },
        "dump": {
          "command": "pnpm xdev db dump --cwd apps/local-first/sync-server"
        },
        "execute": {
          "command": "pnpm xdev db execute --cwd apps/local-first/sync-server"
        },
        "dev": {
          "command": "pnpm xdev db migrate dev --cwd apps/local-first/sync-server"
        },
        "reset": {
          "command": "pnpm xdev db migrate reset --cwd apps/local-first/sync-server"
        },
        "deploy": {
          "command": "pnpm xdev db migrate deploy --cwd apps/local-first/sync-server"
        },
        "resolve": {
          "command": "pnpm xdev db migrate resolve --cwd apps/local-first/sync-server"
        }
      }
    }
  },
  "tags": ["worker"]
}

{
  "name": "template-web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "implicitDependencies": ["template-shared", "template-client"],
  "targets": {
    "app-build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/apps/web"],
      "options": {
        "command": "pnpm xdev build --cwd apps/web"
      }
    },
    "app-deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm xdev deploy --cwd apps/web"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm xdev serve --cwd apps/web"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm xdev --tsconfig=apps/web/tsconfig.app.json preview --cwd apps/web"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/node_modules/.vitest/apps/web", "{workspaceRoot}/coverage/apps/web"],
      "options": {
        "command": "pnpm xdev test --project template-web"
      }
    },
    "madge": {
      "executor": "nx:run-commands",
      "options": {
        "command": "madge --ts-config ./tsconfig.app.json --circular --no-color --no-spinner --extensions ts,tsx .",
        "cwd": "apps/web"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsgo -p apps/web/tsconfig.check.json"
      }
    }
  },
  "tags": []
}

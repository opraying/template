{
  "name": "template-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/client",
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/node_modules/.vitest/apps/client", "{workspaceRoot}/coverage/apps/client"],
      "options": {
        "command": "xdev test --project template-client"
      }
    },
    "madge": {
      "executor": "nx:run-commands",
      "options": {
        "command": "madge --ts-config ./tsconfig.app.json --circular --no-color --no-spinner --extensions ts,tsx .",
        "cwd": "apps/client"
      }
    },
    "db": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "seed",
      "configurations": {
        "seed": {
          "command": "xdev db seed --cwd apps/client"
        },
        "push": {
          "command": "xdev db push --cwd apps/client"
        },
        "dump": {
          "command": "xdev db dump --cwd apps/client"
        },
        "execute": {
          "command": "xdev db execute --cwd apps/client"
        },
        "dev": {
          "command": "xdev db migrate dev --cwd apps/client"
        },
        "reset": {
          "command": "xdev db migrate reset --cwd apps/client"
        },
        "deploy": {
          "command": "xdev db migrate deploy --cwd apps/client"
        },
        "resolve": {
          "command": "xdev db migrate resolve --cwd apps/client"
        }
      }
    }
  }
}
